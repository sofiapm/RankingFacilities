<style type="text/css">
${demo.css}
		</style>
		<script type="text/javascript">
$(function () {

    $('#<%=@v.object_id.to_s + "linecolumngraph" %>').highcharts({
        title: {
            text: '<%= @v[:my_facility_results][:name] + ": " + @v[:my_facility_results][:year].to_s %>'
        },
        xAxis: {
            categories: ['Quarter 1', 'Quarter 2', 'Quarter 3', 'Quarter 4']
        },
 
        labels: {
            items: [{
                html: '',
                style: {
                    left: '50px',
                    top: '18px',
                    color: (Highcharts.theme && Highcharts.theme.textColor) || 'black'
                }
            }]
        },
        series: [{
            type: 'column',
            name: '<%= @v[:my_facility_results][:facility_name] %>',
            data: <%= @v[:my_facility_results][:values] %>,
            color: Highcharts.getOptions().colors[2]
            
        }, {
            type: 'spline',
            name: 'Best Ranked Facility',
            data: <%= @v[:best_facility_results] %>,
            marker: {
                lineWidth: 2,
                lineColor: Highcharts.getOptions().colors[0],
                fillColor: 'black'
            }
        }
        ]
    });
});


</script>
<div class="container col-md-12">
    <div id='<%=@v.object_id.to_s + "linecolumngraph" %>' style="min-width: 290px; height: 400px; margin: 0 auto"></div>
    <div class="container col-md-12 graph"><hr class="featurette-divider"><br /></div>
</div>